# Google Gemini API Configuration
# Получите API ключ на https://aistudio.google.com/app/apikey
AI_BA_GEMINI_API_KEY=your-gemini-api-key-here

# Model Provider (ollama, gemini, transformers)
AI_BA_MODEL_PROVIDER=gemini

# Gemini Model Name (optional, по умолчанию gemini-2.0-flash-exp)
AI_BA_GEMINI_MODEL_NAME=gemini-2.0-flash-exp

# Model Settings (optional)
AI_BA_MODEL_TEMPERATURE=0.2
AI_BA_MODEL_MAX_NEW_TOKENS=2048

# Logging
AI_BA_LOG_LEVEL=INFO

# Ollama Configuration (если используете Ollama вместо Gemini)
# AI_BA_OLLAMA_URL=http://host.docker.internal:11434/api/generate
# AI_BA_MODEL_NAME=gemma:latest

# GitLab Configuration
# Получите Personal Access Token в GitLab: Settings -> Access Tokens
# Нужны права: api, write_repository
GITLAB_ACCESS_TOKEN=your_gitlab_personal_access_token_here
GITLAB_API_URL=https://gitlab.com/api/v4
# GITLAB_WEBHOOK_SECRET_TOKEN=опционально_если_нужен_секретный_токен

# Gemini API Configuration
# Получите API ключ на: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL_NAME=gemini-2.5-flash
GEMINI_TEMPERATURE=0.2
GEMINI_TOP_P=0.9
GEMINI_MAX_TOKENS=8192

# Server Configuration
HOST=0.0.0.0
PORT=8001
DEBUG=false
